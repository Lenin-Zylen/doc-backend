{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_name": {
      "type": "string"
    },
    "product_description": {
      "type": "string"
    },
    "product_icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "releases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::release.release",
      "mappedBy": "product"
    }
  }
}
